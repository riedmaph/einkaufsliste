            "type": "array",
            "items": {
                "$ref": "#/definitions/BrandTransformed"
            }
        },
        "BrandTransformed": {
            "type": "object",
            "required": [
                "id",
                "brand",
                "likelihood"
            ],
            "properties": {
                "id": {
                    "type": "integer",
